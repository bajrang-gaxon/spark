{
  "name": "spark",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "turbo run dev",
    "dev:next": "pnpm --filter spark-ui-next dev",
    "build": "turbo run build --filter=@spark/*",
    "start": "turbo run start",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "turbo run format",
    "format:check": "prettier --check packages/**/**/src --cache",
    "format:write": "prettier --write packages/**/**/src --cache",
    "clean": "turbo run clean",
    "clean:node_modules": "rimraf ./packages/**/**/node_modules && rimraf ./node_modules",
    "clean:all": "pnpm clean && rimraf ./apps/**/node_modules && rimraf ./packages/**/**/node_modules && rimraf ./.turbo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.1",
  "devDependencies": {
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "turbo": "^2.5.0",
    "typescript": "^5"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  }
