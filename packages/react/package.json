{
  "name": "@gxdev/react",
  "version": "1.0.0",
  "description": "ğŸš€ Beautiful and modern React UI library.",
  "license": "MIT",
  "private": false,
  "author": "",
  "keywords": [
    "next",
    "next ui",
    "hero ui",
    "components",
    "modern components",
    "react components",
    "react ui"
  ],
  "main": "dist/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --dts",
    "dev": "pnpm tsup --dts --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.7",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "framer-motion": "^12.7.3",
    "tailwind-merge": "^3.2.0"
  },
  "peerDependencies": {
    "react": ">=18 || >=19.0.0-rc.0",
    "react-dom": ">=18 || >=19.0.0-rc.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "packageManager": "pnpm@10.8.1",
  "clean-package": {
    "remove": ["devDependencies"]
     }
}
