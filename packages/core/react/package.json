{
    "name": "@spark/react",
    "version": "0.0.0",
    "description": "",
    "author": "",
    "license": "MIT",
    "sideEffects": false,
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts": {
      "build": "pnpm build:cjs && pnpm build:dts && pnpm copy && pnpm move:dist",
      "copy": "cp ./package.json ./dist",
      "clean": "rimraf ./dist ./.turbo",
      "build:cjs": "swc ./src --out-dir ./dist --config-file ../../../.swcrc",
      "build:dts": "tsc --declaration --declarationMap --emitDeclarationOnly",
      "move:dist": "shx mv ./dist/src/* ./dist/ && shx rm -r ./dist/src",
      "format:check": "prettier --check src --cache",
      "format:write": "prettier --write src --cache",
      "typecheck": "tsc --noEmit"
     },
    "keywords": [],
    "packageManager": "pnpm@10.8.1",
    "dependencies": {
      "@spark/button": "workspace:*"
    },
    "peerDependencies": {
        "react": ">=18 || >=19",
        "react-dom": ">=18 || >=19"
      },
   "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }  

   
