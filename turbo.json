{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**", "dist/**"]
      },
      "typecheck": {
        "cache": false,
        "dependsOn": ["^typecheck"]
      },
      "start": {
        "dependsOn": ["^build"],
        "outputs": []
      },
      "test": {
        "dependsOn": ["build"],
        "outputs": []
      },
      "clean": {
        "cache": false,
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false,
        "dependsOn": ["^dev"],
        "outputs": ["dist/**", ".next/**"]
      }
    }
  }
  